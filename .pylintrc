[MASTER]
ignore=venv,taskmanager-env
jobs=1

[MESSAGES CONTROL]
disable=C0114,C0115,C0116,R0903,R0902,R0913,R0914,R0915

[FORMAT]
max-line-length=100

[DESIGN]
max-args=6
max-locals=20
max-branches=10
max-statements=50
max-parents=7
max-attributes=10
max-public-methods=20

[REPORTS]
output-format=colorized
reports=no

[TYPECHECK]
generated-members=numpy.*,torch.*
